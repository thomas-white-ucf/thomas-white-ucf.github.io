  "description": "development portfolio",
  "main": "src",
  "repository": "git@github.com:thomas-white-ucf/thomas-white-ucf.github.io.git",
  "license": "The Unlicense",
  "version": "1.0.0",
  "private": false,
  "keywords": [
    "heroku",
    "portfolio"
  ],
  "scripts": {
    "start": "react-scripts start",
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "build-debug": "react-scripts build --debug",
    "build-p": "react-scripts build --profile",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "u": "yarn upgrade --latest",
    "u-i": "yarn upgrade-interactive --latest",
    "i-check": "yarn install --check-files",
    "lint": "react-scripts lint",
    "test": "echo \"Error: >> no test specified\" && exit 1",
    "serve": "serve -s build",
    "add-commit": "git add . && git commit -m 'auto'"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.6.3",
    "gsap": "^3.10.4",
    "nth-check": "^2.0.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1"
  },
  "engines": {
    "node": "16.x",
    "yarn": "1.x",
    "npm": "8.x"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^8.14.0",
    "gh-pages": "^3.2.3",
    "serve": "^13.0.2",
    "yarn-upgrade-all": "^0.7.1"
  }
}
